[gd_resource type="Resource" load_steps=2 format=2]

[sub_resource type="GDScript" id=1]
script/source = "extends Command

func _execute(_arguments: Array) -> void:
	var target_module: String 
	if _arguments.size() == 0:
		target_module = ResourceManager.master_module
	else:
		target_module = _arguments[0]
	var module = ResourceManager.get_module(target_module)
	if module == null:
		Console.error(\"Namespace \\\"\" + target_module + \"\\\" not found\" )
		return
	var output: String = \"Command Help:\"
	for command in module.command_list:
		if not command.description.empty():
			output += \"\\n[color=lime]\" + command.name + \"[/color] - \" + command.description
	Console.print(output)
"

[resource]
script = SubResource( 1 )
arguments = [ 0, 1 ]
description = "Display this message."
