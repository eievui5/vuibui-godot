[gd_resource type="Resource" load_steps=2 format=2]

[sub_resource type="GDScript" id=1]
script/source = "extends Command

func _execute(_arguments: Array) -> void:
	for entity in ResourceManager.entity_list:
		if _arguments[0] == entity.get_file().replace(
				\".\" + entity.get_extension(), \"\"):
			var new_entity: Entity = load(entity).instance()
			Console.get_tree().root.add_child(new_entity)
			Console.print(\"Summoned \" + _arguments[0] + \".\")
			return
	Console.error(\"Entity \\\"\" + _arguments[0] + \"\\\" not found.\")
"

[resource]
script = SubResource( 1 )
arguments = [ 1 ]
description = "Summon an entity."
